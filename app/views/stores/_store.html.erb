<%= turbo_frame_tag dom_id(store) do %>
    <tr>
      <td><%= link_to store.name, store, class: "list" %></td>
      <td><%= link_to 'Edit', edit_store_path(store), class:"btn btn-warning" %></td>
      <td><%= link_to 'Destroy', store, method: :delete, class:"btn btn-danger" %></td>
    </tr>
<% end %>
